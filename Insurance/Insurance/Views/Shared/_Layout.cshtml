                                        </svg>保单列表
                                    </a>
                                }
                            }
                        </li>
                        @{

                            if (Context.Session.Get<UserInfoModel>("CurrentUser").CompanyName != string.Empty &&
                                Context.Session.Get<UserInfoModel>("CurrentUser").CompanyName != "管理员")
                            {
