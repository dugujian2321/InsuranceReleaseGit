                                        </svg>保单列表
                                    </a>
                                }
                                else
                                {
                                    string str = Context.Session.Get<UserInfoModel>("CurrentUser").CompanyName;
                                    <a id="page_2" class="nav-link" href="/Home/CompanyHisitoryByMonth?name=@str" data-ajax="true" data-ajax-method="GET" data-ajax-mode="replace" data-ajax-update="#content">
                                        <svg width="1em" height="1em" viewBox="0 0 16 16" class="bi bi-receipt tagpic" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
